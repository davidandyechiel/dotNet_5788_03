        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dotNet_5778_03_0520_0473"
        mc:Ignorable="d"
        Title="InkManager" Height="122" Width="300">
    <Grid Background="#FFE4E4E4">
        <Image x:Name="No_Ink" HorizontalAlignment="Left" Height="60" Margin="20,15,0,0" VerticalAlignment="Top" Width="55" Source="images/no ink.jpg" RenderTransformOrigin="0.533,0.583"/>
        <Button x:Name="button" Content="Add Ink" HorizontalAlignment="Left" Margin="125,65,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Margin="115,15,0,0" VerticalAlignment="Top" Height="45" Width="160"/>
        <Image x:Name="Critical_Img" HorizontalAlignment="Left" Height="91" Margin="-20,-25,0,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Window>
